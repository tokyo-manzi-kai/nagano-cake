<div class='container'>
  <div class='row mt-3'>
    <div class="col-md-3 bg-light m-4 text-center">
      <h2>会員情報編集</h2>
    </div>
  </div>

  <div class="row">
    <div class='col-md-12 mx-auto mt-3'>
     <%= form_with model:@end_user, url: users_path do |f| %>
      <div class="row form-group">
        <%= f.label "氏名" %>
        <%= f.text_field @end_user.first_name %>
        <%= f.text_field @end_user.last_name %>
      </div>

      <div class="row form-group">
        <%= f.label "フリガナ" %>
        <%= f.text_field @end_user.first_name_kana %>
        <%= f.text_field @end_user.last_name_kana %>
      </div>

      <div class="row form-group">
        <%= f.label "郵便番号" %>
        <%= f.text_field @end_user.post_code %>
      </div>

      <div class="row form-group">
        <%= f.label "住所" %>
        <%= f.text_field @end_user.address %>
      </div>

      <div class="row form-group">
        <%= f.label "電話番号" %>
        <%= f.text_field @end_user.phone_number %>
      </div>

      <div class="row form-group">
        <%= f.label "メールアドレス" %>
        <%= f.text_field @end_user.email %>
      </div>

      <%= f.submit "編集内容を保存" %>

      <%= f.submit "退会する" %>

     <% end %>

    </div>
  </div>
</div>

